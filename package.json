{
    "name": "gnome-shell-memento-mori",
    "version": "7",
    "scripts": {
        "build": "npm run build:ts && npm run build:extension",
        "build:ts": "npm run clean && npx rollup -c --configPlugin typescript",
        "clean": "rm -rf dist",
        "build:extension": "npm run gen:locale && npm run build:schema",
        "build:schema": "glib-compile-schemas resources/schemas --targetdir=dist/schemas",
        "gen:locale": "npx ts-node scripts/generateLocale.ts",
        "watch": "npm run build && npx rollup -c --configPlugin typescript --watch",
        "lint": "eslint ."
    },
    "devDependencies": {
        "@gi-types/adw1": "1.1.1",
        "@gi-types/clutter10": "10.0.1",
        "@gi-types/gio2": "2.72.1",
        "@gi-types/glib2": "2.72.1",
        "@gi-types/gobject2": "2.72.1",
        "@gi-types/gtk4": "4.6.1",
        "@gi-types/st1": "1.0.1",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-typescript": "11.0.0",
        "@tsconfig/strictest": "1.0.2",
        "@types/gettext-parser": "4.0.2",
        "@types/glob": "8.1.0",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "@typescript-eslint/parser": "5.54.0",
        "date-fns": "2.29.3",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-prettier": "4.2.1",
        "fill-pot-po": "2.1.13",
        "gettext-extractor": "3.6.2",
        "gettext-parser": "6.0.0",
        "glob": "9.2.1",
        "prettier": "2.8.4",
        "rollup": "3.18.0",
        "rollup-plugin-cleanup": "3.2.1",
        "rollup-plugin-copy": "3.4.0",
        "ts-dedent": "2.2.0",
        "ts-node": "10.9.1",
        "tslib": "2.5.0",
        "typescript": "4.9.5"
    },
    "packageManager": "yarn@3.4.1"
}
